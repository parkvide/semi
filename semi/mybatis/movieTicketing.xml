<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="MemberMapper">

<select id="MovieMapper.selectMovieList" resultType="com.kh.app.movie.vo.MovieVo">
	 	SELECT M.TITLE 
	 	FROM SCREEN_IF SI JOIN MOVIE M
	 	ON  SI.MOVIE_NO = M.NO
	 	WHERE M.DEL_YN = 'N'
	</select>
	
	</mapper>